{
    "index_patterns": [
      "weblogs"
    ],
    "priority": 1000,
    "template": {
      "settings": {
        "index": {
          "number_of_shards": "1",
          "number_of_replicas": "0"
        }
      },
      "mappings": {
        "dynamic_templates": [],
        "properties": {
          "server.geo.country_iso_code": {
            "type": "keyword"
          },
          "http.request.referer": {
            "type": "keyword"
          },
          "server.memory": {
            "type": "long"
          },
          "server.ip": {
            "type": "ip"
          },
          "server.phpmemory": {
            "type": "long"
          },
          "client.geo.country_iso_code": {
            "type": "keyword"
          },
          "client.ip": {
            "type": "ip"
          },
          "host.hostname": {
            "type": "keyword"
          },
          "tags": {
            "type": "keyword"
          },
          "@timestamp": {
            "type": "date"
          },
          "url.path": {
            "type": "keyword"
          },
          "client.geo.location": {
            "type": "geo_point"
          },
          "http.response.bytes": {
            "type": "long"
          },
          "machine.ram": {
            "type": "long"
          },
          "file.extension": {
            "type": "keyword"
          },
          "http.response.status_code": {
            "type": "keyword"
          },
          "url.full": {
            "type": "keyword"
          },
          "user_agent": {
            "type": "object",
            "properties": {
              "original": {
                "type": "text",
                "fields": {
                  "keyword": {
                    "ignore_above": 256,
                    "type": "keyword"
                  }
                }
              },
              "os": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "keyword"
                  },
                  "version": {
                    "type": "keyword"
                  },
                  "full": {
                    "type": "keyword"
                  }
                }
              },
              "name": {
                "type": "keyword"
              },
              "device": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "keyword"
                  }
                }
              },
              "version": {
                "type": "keyword"
              }
            }
          },
          "timestamp": {
            "format": "iso8601",
            "type": "date_nanos"
          }
        }
      }
    }
  }